{
  "name": "ink-mde",
  "type": "module",
  "version": "0.18.1",
  "packageManager": "pnpm@7.25.1",
  "description": "The flexible TypeScript Markdown editor that powers https://octo.app.",
  "author": "David R. Myers <david@voracious.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/voracious",
  "homepage": "https://github.com/voracious/ink-mde",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voracious/ink-mde.git"
  },
  "bugs": {
    "url": "https://github.com/voracious/ink-mde/issues"
  },
  "keywords": [
    "codemirror",
    "component",
    "easymde",
    "javascript",
    "js",
    "markdown",
    "mde",
    "octo",
    "sfc",
    "simplemde",
    "ts",
    "typescript",
    "vue",
    "vue3"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": {
        "types": "./types/index.d.ts",
        "require": "./dist/client.cjs",
        "import": "./dist/client.js"
      },
      "node": {
        "types": "./types/index.d.ts",
        "require": "./dist/server.cjs",
        "import": "./dist/server.js"
      },
      "types": "./types/index.d.ts",
      "require": "./dist/client.cjs",
      "import": "./dist/client.js"
    },
    "./svelte": {
      "types": "./svelte/package/index.d.ts",
      "import": "./svelte/package/index.js"
    },
    "./vue": {
      "browser": {
        "types": "./vue/dist/index.d.ts",
        "require": "./vue/dist/client.cjs",
        "import": "./vue/dist/client.js"
      },
      "node": {
        "types": "./vue/dist/index.d.ts",
        "require": "./vue/dist/server.cjs",
        "import": "./vue/dist/server.js"
      },
      "types": "./vue/dist/index.d.ts",
      "require": "./vue/dist/client.cjs",
      "import": "./vue/dist/client.js"
    }
  },
  "browser": {
    "./dist/server.cjs": "./dist/client.cjs",
    "./dist/server.js": "./dist/client.js"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "types/**/*",
    "vue/**/*"
  ],
  "scripts": {
    "build": "pnpm build:root && pnpm vue:build",
    "build:root": "tsc && vite build && VITE_SSR=1 vite build --ssr ./src/index.tsx && tsc --project ./tsconfig.types.json && cp ./types/ink.d.ts ./dist/index.d.ts && mv ./dist/index.js ./dist/server.js && mv ./dist/index.cjs ./dist/server.cjs",
    "ci": "pnpm build && pnpm test:once && pnpm lint && pnpm docs:check && pnpm dedupe:check",
    "dedupe": "pnpm-deduplicate",
    "dedupe:check": "pnpm-deduplicate --list",
    "dev": "vite",
    "docs:check": "embedme --verify README.md",
    "docs:diff": "embedme --stdout README.md",
    "docs:update": "embedme README.md",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepack": "pnpm build",
    "test": "vitest",
    "test:once": "vitest run",
    "vue:build": "vue-tsc --project ./vue/tsconfig.json && vite build -c ./vue/vite.config.ts && VITE_SSR=1 vite build -c ./vue/vite.config.ts --ssr ./vue/src/InkMde.vue && vue-tsc --project ./vue/tsconfig.types.json && mv ./vue/dist/InkMde.vue.d.ts ./vue/dist/index.d.ts && mv ./vue/dist/InkMde.js ./vue/dist/server.js && mv ./vue/dist/InkMde.cjs ./vue/dist/server.cjs",
    "vue:dev": "tsx ./vue/server.ts"
  },
  "peerDependencies": {
    "svelte": "^3.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.4.0",
    "@codemirror/commands": "^6.2.0",
    "@codemirror/lang-markdown": "^6.0.5",
    "@codemirror/language": "^6.4.0",
    "@codemirror/language-data": "^6.1.0",
    "@codemirror/search": "^6.2.3",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.7.3",
    "@lezer/common": "^1.0.2",
    "@lezer/highlight": "^1.1.3",
    "@lezer/markdown": "^1.0.2",
    "@replit/codemirror-vim": "^6.0.4",
    "ink-mde": "workspace:*",
    "solid-js": "^1.6.9",
    "style-mod": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.34.1",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "embedme": "github:voracious/embedme#live-fork",
    "eslint": "^8.32.0",
    "eslint-plugin-solid": "^0.7.4",
    "express": "^4.18.2",
    "jsdom": "^20.0.3",
    "pnpm-deduplicate": "^0.4.1",
    "tsx": "^3.12.2",
    "typescript": "~4.8.4",
    "vite": "^4.0.4",
    "vite-plugin-externalize-deps": "^0.5.0",
    "vite-plugin-solid": "2.5.0",
    "vitest": "^0.28.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  },
  "publishConfig": {
    "access": "public"
  }
}
